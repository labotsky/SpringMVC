#parse("layout/main.vm")
#@main()
<div class="container">	
		<div class="row">
			<form action="#springUrl("/category/add")" method="POST"> 
		        <table>
		            <tr>
		                <td>Name:#springFormInput("category.name","")</td>
		            </tr>
		            <tr>
		                <td>Description:#springFormInput("category.description","")</td>
		            </tr>
		        </table>
		       <button type="submit" class="btn btn-default">Submit</button>
    		</form>					
		</div>
	</div>
#end